{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/types": "^7.0.25",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.36",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^8.2.0",
    "i18next": "^23.2.6",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-overlay": "^6.0.11",
    "react-i18next": "^13.0.1",
    "react-router-dom": "^6.14.0",
    "react-scripts": "5.0.1",
    "sass": "^1.63.6",
    "source-map-explorer": "^2.5.3",
    "styled-components": "^6.0.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "unit:test": "react-scripts test",
    "unit:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "server": "npm --prefix ../api npm run start:dev",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "loki": "npx loki",
    "loki:test": "loki test",
    "loki:appprove": "loki approve",
    "test:unit": "react-scripts test --watchAll=false",
    "test:loki": "loki test",
    "test:build-storybook": "storybook build -s ./",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.25",
    "@storybook/addon-interactions": "^7.0.25",
    "@storybook/addon-links": "^7.0.25",
    "@storybook/blocks": "^7.0.25",
    "@storybook/preset-create-react-app": "^7.0.25",
    "@storybook/react": "^7.0.25",
    "@storybook/react-webpack5": "^7.0.25",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/addon-docs": "^7.0.26",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-plugin-storybook": "^0.6.12",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.25",
    "webpack": "^5.88.1"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
